{
  "name": "han-prev-cli",
  "version": "1.0.36",
  "description": "A powerful CLI for Han Framework development",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "han": "./bin/han.js"
  },
  "scripts": {
    "build": "npm run format && rimraf dist && tsc",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "han",
    "framework",
    "cli",
    "nestjs",
    "typescript",
    "nodejs"
  ],
  "author": "Han Framework Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/han-framework.git",
    "directory": "packages/cli"
  },
  "homepage": "https://han-framework.dev",
  "bugs": {
    "url": "https://github.com/your-org/han-framework/issues"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "dependencies": {
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "figlet": "^1.6.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.6",
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^8.2.6",
    "@types/node": "^20.0.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
